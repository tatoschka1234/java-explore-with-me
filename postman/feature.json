							"    let updateText = \"Отредактированный текст комментария\";\r",
							"    try {\r",
							"        const user = await api.addUser(rnd.getUser());\r",
							"        pm.collectionVariables.set(\"uid\", user.id);\r",
							"\r",
							"        const category = await api.addCategory(rnd.getCategory());\r",
							"        pm.collectionVariables.set(\"cid\", category.id);\r",
							"\r",
							"        const event = await api.publishEvent(\r",
							"            (await api.addEvent(user.id, rnd.getEvent(category.id))).id\r",
							"        );\r",
							"        pm.collectionVariables.set(\"eid\", event.id);\r",
							"\r",
							"        const comment = await api.addComment(user.id, event.id, rnd.getSentence(4));\r",
							"        pm.collectionVariables.set(\"comid\", comment.id);\r",
							"\r",
							"        pm.collectionVariables.set('updateText', updateText);\r",
							"\r",
							"    } catch (err) {\r",
							"        console.error(\"Ошибка при подготовке тестовых данных.\", err);\r",
							"    }\r",
							"\r",
							"    pm.request.body.update({\r",
							"        mode: 'raw',\r",
							"        raw: JSON.stringify({ text: updateText }),\r",
							"        options: { raw: { language: 'json' } }\r",
							"    });\r",
							"    console.log(\"Request body: \", pm.request.body.raw);\r",
							"};\r",
							"\r",
							"const interval = setInterval(() => {}, 1000);\r",
							"\r",
							"setTimeout(async () => {\r",
							"    try {\r",
							"        await main();\r",
							"    } catch (e) {\r",
							"        console.error(e);\r",
							"    } finally {\r",
							"        clearInterval(interval);\r",
							"    }\r",
							"}, 100);\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Ответ должен содержать код статуса 200 и данные в формате json\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"    pm.response.to.be.withBody;\r",
							"    pm.response.to.be.json;\r",
							"});\r",
							"\r",
							"const expectedText = pm.collectionVariables.get('updateText');\r",
							"const actual = pm.response.json();\r",
							"\r",
							"pm.test(\"Текст комментария должен совпадать с отправленным\", function () {\r",
							"    pm.expect(actual.text).to.eql(expectedText);\r",
							"});"
